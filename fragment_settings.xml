<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ui_background"
    tools:context=".ui.Settings.FragmentSettings$FragmentSettings" >

    <TextView
        android:id="@+id/text_settings"
        android:layout_width="339dp"
        android:layout_height="46dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="13dp"
        android:fontFamily="@font/luxury_modish"
        android:padding="2dp"
        android:text="Settings"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/view11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="13dp"
        android:background="@drawable/button_backgrund"
        android:src="@drawable/baseline_arrow_back"
        app:layout_constraintBottom_toTopOf="@+id/view11"
        app:layout_constraintEnd_toStartOf="@+id/text_settings"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/view11"
        android:layout_width="387dp"
        android:layout_height="40dp"
        android:drawableLeft="@drawable/language_24"
        android:padding="9dp"
        android:text="    Language"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="113dp" />

    <TextView
        android:id="@+id/view12"
        android:layout_width="387dp"
        android:layout_height="40dp"
        android:drawableLeft="@drawable/person_24"
        android:padding="9dp"
        android:text="    My Profile"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="167dp" />


</androidx.constraintlayout.widget.ConstraintLayout>